unit View.Cadastro.Endereco;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, View.Cadastro, Data.DB, Vcl.StdCtrls, Vcl.Buttons, Vcl.ExtCtrls, Vcl.Grids, Vcl.DBGrids;

type
  TfrmCadastroEndereco = class(TfrmCadastro)
    procedure FormCreate(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  frmCadastroEndereco: TfrmCadastroEndereco;

implementation

uses
  Model.Cadastro, View.Cadastro.EnderecoDetalhe;

{$R *.dfm}

procedure TfrmCadastroEndereco.FormCreate(Sender: TObject);
begin
  inherited;
  DataSetLocal := dmCadastro.qryCadastroEmpresa;
  FormDetailClass := TfrmCadastroEnderecoDetalhe;
  FormDetail := frmCadastroEnderecoDetalhe;
end;

end.
