  object srcCadastroFuncionario: TDataSource
    DataSet = qryCadastroFuncionario
    Left = 506
    Top = 190
  end
  object qryCadastroFuncionario: TFDQuery
    Connection = dmConexao.Connection
    UpdateObject = upCadastroFuncionario
    SQL.Strings = (
      'SELECT '
      '  F.IDFUNCIONARIO,'
      '  F.IDEMPRESA,'
      '  E.NMEMPRESA,'
      '  F.NMFUNCIONARIO,'
      '  F.NUCPF,'
      '  F.NURG,'
      '  F.DTNASCIMENTO,'
      '  F.TXEMAIL,'
      '  F.NUCARTEIRATRAB,'
      '  F.NUTITULOELEITOR,'
      '  F.NUCARTEIRAMOTORISTA,'
      '  F.TPCATERORIA,'
      '  F.DTVALIDADECARTEIRAMOT,'
      '  F.TLRESIDENCIAL,'
      '  F.TLCELULAR,'
      '  F.TLCONTATO,'
      '  F.NMCONTATO,'
      '  F.DTCONTRATACAO,'
      '  F.DTDEMISSAO,'
      '  F.DTCADASTRO,'
      '  F.STATIVO,'
      '  F.TXOBS,'
      '  F.NMENDERECO,'
      '  F.NUENDERECO,'
      '  F.NMBAIRRO,'
      '  F.IDCIDADE,'
      '  C.NMCIDADE,'
      '  F.IDUF,'
      '  U.SGESTADO, '
      '  F.NUCEP,'
      '  F.STEXCLUIDO,'
      '  F.DTEXCLUIDO'
      'FROM'
      '  CADFUNCIONARIOS F'
      '  LEFT JOIN CADEMPRESA E ON F.IDEMPRESA = E.IDEMPRESA '
      '  LEFT JOIN CADCIDADE C ON F.IDCIDADE = C.IDCIDADE '
      '  LEFT JOIN CADUF U ON F.IDUF = U.IDUF'
      'WHERE'
      '  F.STEXCLUIDO = '#39'N'#39
      'ORDER BY '
      '  F.NMFUNCIONARIO')
    Left = 505
    Top = 18
  end
  object upCadastroFuncionario: TFDUpdateSQL
    Connection = dmConexao.Connection
    InsertSQL.Strings = (
      'INSERT INTO CADFUNCIONARIOS'
      '(IDFUNCIONARIO, IDEMPRESA, NMFUNCIONARIO, NUCPF, '
      '  NURG, DTNASCIMENTO, TXEMAIL, NUCARTEIRATRAB, '
      '  NUTITULOELEITOR, NUCARTEIRAMOTORISTA, TPCATERORIA, '
      '  DTVALIDADECARTEIRAMOT, TLRESIDENCIAL, TLCELULAR, '
      '  TLCONTATO, NMCONTATO, DTCONTRATACAO, DTDEMISSAO, '
      '  DTCADASTRO, STATIVO, NMENDERECO, '
      '  NUENDERECO, NMBAIRRO, IDCIDADE, IDUF, '
      '  NUCEP, STEXCLUIDO)'
      
        'VALUES (:NEW_IDFUNCIONARIO, :NEW_IDEMPRESA, :NEW_NMFUNCIONARIO, ' +
        ':NEW_NUCPF, '
      
        '  :NEW_NURG, :NEW_DTNASCIMENTO, :NEW_TXEMAIL, :NEW_NUCARTEIRATRA' +
        'B, '
      
        '  :NEW_NUTITULOELEITOR, :NEW_NUCARTEIRAMOTORISTA, :NEW_TPCATEROR' +
        'IA, '
      
        '  :NEW_DTVALIDADECARTEIRAMOT, :NEW_TLRESIDENCIAL, :NEW_TLCELULAR' +
        ', '
      
        '  :NEW_TLCONTATO, :NEW_NMCONTATO, :NEW_DTCONTRATACAO, :NEW_DTDEM' +
        'ISSAO, '
      '  CURRENT_DATE, '#39'S'#39', :NEW_NMENDERECO, '
      '  :NEW_NUENDERECO, :NEW_NMBAIRRO, :NEW_IDCIDADE, :NEW_IDUF, '
      '  :NEW_NUCEP, '#39'N'#39')')
    ModifySQL.Strings = (
      'UPDATE CADFUNCIONARIOS'
      'SET IDEMPRESA = :NEW_IDEMPRESA, '
      '  NMFUNCIONARIO = :NEW_NMFUNCIONARIO, NUCPF = :NEW_NUCPF, '
      
        '  NURG = :NEW_NURG, DTNASCIMENTO = :NEW_DTNASCIMENTO, TXEMAIL = ' +
        ':NEW_TXEMAIL, '
      
        '  NUCARTEIRATRAB = :NEW_NUCARTEIRATRAB, NUTITULOELEITOR = :NEW_N' +
        'UTITULOELEITOR, '
      '  NUCARTEIRAMOTORISTA = :NEW_NUCARTEIRAMOTORISTA, '
      
        '  DTVALIDADECARTEIRAMOT = :NEW_DTVALIDADECARTEIRAMOT, TLRESIDENC' +
        'IAL = :NEW_TLRESIDENCIAL, '
      '  TLCELULAR = :NEW_TLCELULAR, TLCONTATO = :NEW_TLCONTATO, '
      
        '  NMCONTATO = :NEW_NMCONTATO, DTCONTRATACAO = :NEW_DTCONTRATACAO' +
        ', '
      '  DTDEMISSAO = :NEW_DTDEMISSAO, '
      '  STATIVO = :NEW_STATIVO, NMENDERECO = :NEW_NMENDERECO, '
      '  NUENDERECO = :NEW_NUENDERECO, NMBAIRRO = :NEW_NMBAIRRO, '
      '  IDCIDADE = :NEW_IDCIDADE, IDUF = :NEW_IDUF, NUCEP = :NEW_NUCEP'
      'WHERE IDFUNCIONARIO = :OLD_IDFUNCIONARIO')
    DeleteSQL.Strings = (
      'UPDATE CADFUNCIONARIOS'
      'SET STEXCLUIDO = '#39'S'#39', DTEXCLUIDO = CURRENT_DATE'
      'WHERE IDFUNCIONARIO = :OLD_IDFUNCIONARIO')
    FetchRowSQL.Strings = (
      
        'SELECT IDFUNCIONARIO, IDEMPRESA, NMFUNCIONARIO, NUCPF, NURG, DTN' +
        'ASCIMENTO, '
      
        '  TXEMAIL, NUCARTEIRATRAB, NUTITULOELEITOR, NUCARTEIRAMOTORISTA,' +
        ' '
      '  TPCATERORIA, DTVALIDADECARTEIRAMOT, TLRESIDENCIAL, TLCELULAR, '
      '  TLCONTATO, NMCONTATO, DTCONTRATACAO, DTDEMISSAO, DTCADASTRO, '
      '  STATIVO, TXOBS, NMENDERECO, NUENDERECO, NMBAIRRO, IDCIDADE, '
      '  IDUF, NUCEP, STEXCLUIDO, DTEXCLUIDO'
      'FROM CADFUNCIONARIOS'
      'WHERE IDFUNCIONARIO = :OLD_IDFUNCIONARIO')
    Left = 505
    Top = 104
  end
end
