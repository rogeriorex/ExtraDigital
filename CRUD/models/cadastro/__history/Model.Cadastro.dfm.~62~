    SQL.Strings = (
      'SELECT IDTITULAR, NMTITULAR'
      'FROM   CADTITULAR'
      'WHERE STEXCLUIDO <> '#39'S'#39
      'ORDER BY NMTITULAR')
    Left = 665
    Top = 18
